###############################################################################
##  All configurable variables are defined in the file named Makefile.config.in
##  When Autoconf is run, it will create a file named Makefile.config which
##  will have all of the configurable variables replaced with their values.
###############################################################################



srcdir = .
top_srcdir = ..
prefix = /data/OCETest/cm/external/x86_64-linux/intel/intel
exec_prefix = ${prefix}



HYPRE_RELEASE_NAME    = hypre
HYPRE_RELEASE_VERSION = 2.9.0b
HYPRE_RELEASE_DATE    = 2012/10/30
HYPRE_RELEASE_TIME    = 00:00:00
HYPRE_RELEASE_BUGS    = hypre-support@llnl.gov

HYPRE_SRC_TOP_DIR = /data/OCETest/cm/external/packages/Hypre/hypre-2.9.0b/src
HYPRE_BUILD_DIR   = /data/OCETest/cm/external/packages/Hypre/hypre-2.9.0b/src/hypre

HYPRE_INSTALL_DIR = /data/OCETest/cm/external/x86_64-linux/intel/intel
HYPRE_LIB_INSTALL = ${exec_prefix}/lib
HYPRE_INC_INSTALL = ${prefix}/include

HYPRE_LIB_SUFFIX = .a

.SUFFIXES:
.SUFFIXES: .o .f .c .C .cxx .cc

.f.o:
	$(F77) $(FFLAGS) -c $< 
.c.o:
	$(CC) $(CFLAGS) -c $< 
.C.o:
	$(CXX) $(CXXFLAGS) -c $<
.cxx.o:
	$(CXX) $(CXXFLAGS) -c $<
.cc.o:
	$(CXX) $(CXXFLAGS) -c $<

F77       = /opt/intel/impi/4.1.1.036/bin64/mpiifort
FFLAGS    = -O  $(F77_COMPILE_FLAGS)

CC        = /opt/intel/impi/4.1.1.036/bin64/mpiicc
CFLAGS    =   -m64 -fPIC -O3 -DHAVE_CONFIG_H $(C_COMPILE_FLAGS)

CXX       = /opt/intel/impi/4.1.1.036/bin64/mpiicpc
CXXFLAGS  =   -m64 -fPIC -O3 -DHAVE_CONFIG_H $(CXX_COMPILE_FLAGS)

LINK_F77  = /opt/intel/impi/4.1.1.036/bin64/mpiifort
LINK_CC   = /opt/intel/impi/4.1.1.036/bin64/mpiicc
LINK_CXX  = /opt/intel/impi/4.1.1.036/bin64/mpiicpc

BUILD_F77_SHARED    = 
BUILD_CC_SHARED     = 
BUILD_CXX_SHARED    = 
SHARED_COMPILE_FLAG = 
SHARED_BUILD_FLAG   = 
SHARED_SET_SONAME   = 
SHARED_OPTIONS      = 

BUILD_PYTHON = 0
PYTHON       = 

BUILD_JAVA = 0

AR     = ar -rcu
RANLIB = ranlib

LDFLAGS =   -m64 -fPIC -O3
LIBS    =   -lmpi -lmpigi -lrt -ldl -lpthread -lstdc++ -lm
FLIBS   =  -L/opt/intel/impi/4.1.1.036/intel64/lib -L/opt/intel/composer_xe_2013.5.192/compiler/lib/intel64 -L/opt/intel/composer_xe_2013.5.192/mkl/lib/intel64 -L/opt/intel/composer_xe_2013.5.192/ipp/../compiler/lib/intel64 -L/opt/intel/composer_xe_2013.5.192/ipp/lib/intel64 -L/opt/intel/composer_xe_2013.5.192/tbb/lib/intel64/gcc4.4 -L/usr/lib/gcc/x86_64-redhat-linux/4.8.1/ -L/usr/lib/gcc/x86_64-redhat-linux/4.8.1/../../../../lib64 -L/usr/lib/gcc/x86_64-redhat-linux/4.8.1/../../../../lib64/ -L/lib/../lib64 -L/lib/../lib64/ -L/usr/lib/../lib64 -L/usr/lib/../lib64/ -L/opt/intel/composer_xe_2013.5.192/compiler/lib/intel64/ -L/opt/intel/composer_xe_2013.5.192/mkl/lib/intel64/ -L/opt/intel/composer_xe_2013.5.192/ipp/../compiler/lib/intel64/ -L/opt/intel/composer_xe_2013.5.192/ipp/lib/intel64/ -L/opt/intel/composer_xe_2013.5.192/tbb/lib/intel64/gcc4.4/ -L/usr/lib/gcc/x86_64-redhat-linux/4.8.1/../../../ -L/lib64 -L/lib/ -L/usr/lib64 -L/usr/lib -lmpigf -lmpi -lmpigi -ldl -lrt -lpthread -lifport -lifcore -limf -lsvml -lm -lipgo -lirc -lirc_s

##################################################################
##  BABEL Directories
##################################################################
HYPRE_BABEL_DIRS = 

##################################################################
##  Examples Directory
##################################################################
HYPRE_EXAMPLE_DIRS = /data/OCETest/cm/external/packages/Hypre/hypre-2.9.0b/src/examples

##################################################################
##  LAPACK Library Flags 
##################################################################
HYPRE_LAPACK_SRC_DIR = 
LAPACKLIBS           =  -lmkl_rt -lpthread -lm
LAPACKLIBDIRS        = -L/opt/intel/composer_xe_2013.5.192/mkl/lib/intel64 

##################################################################
##  BLAS Library Flags 
##################################################################
HYPRE_BLAS_SRC_DIR = 
BLASLIBS           =  -lmkl_rt -lpthread -lm
BLASLIBDIRS        = -L/opt/intel/composer_xe_2013.5.192/mkl/lib/intel64 

##################################################################
##  MPI options
##################################################################
MPIINCLUDE =  -I/opt/intel/impi/4.1.1.036/include64
MPILIBDIRS = -L/opt/intel/impi/4.1.1.036/lib64 
MPILIBS    =  -lmpi -lmpigi -lrt -ldl -lpthread
MPIFLAGS   = 

##################################################################
##  FEI options
##################################################################
HYPRE_FEI_SRC_DIR       = /data/OCETest/cm/external/packages/Hypre/hypre-2.9.0b/src/FEI_mv
HYPRE_FEI_BASE_DIR      = /data/OCETest/cm/external/packages/Hypre/hypre-2.9.0b/src/FEI_mv/fei-base
HYPRE_FEI_SUBDIRS       = femli fei-hypre
HYPRE_FEI_SUPERLU_FILES = 
HYPRE_FEI_HYPRE_FILES   = /data/OCETest/cm/external/packages/Hypre/hypre-2.9.0b/src/FEI_mv/fei-hypre/*.o
HYPRE_FEI_FEMLI_FILES   = /data/OCETest/cm/external/packages/Hypre/hypre-2.9.0b/src/FEI_mv/femli/*.o
